	option vlan '1'
	option ports '3t 4 5 6'

config switch_vlan
	option device 'switch1'
	option vlan '4'
	option ports '3t 7'

config switch_vlan
	option device 'switch1'
	option vlan '6'
	option ports '3t'

config switch
	option name 'switch0'
	option reset '1'
	option enable_vlan '0'

config interface 'guest'
	option force_link '1'
	option type 'bridge'
	option proto 'static'
	option ipaddr '192.168.9.1'
	option netmask '255.255.255.0'
	option ip6assign '64'
	option multicast_querier '1'
	option igmp_snooping '0'
	option isolate '0'
