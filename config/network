config interface 'loopback'
	option device 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config globals 'globals'
	option ula_prefix 'fd44:9f52:3f1f::/48'

config device
	option name 'br-lan'
	option type 'bridge'
	list ports 'eth1.1'
	option macaddr '94:83:c4:b0:97:44'
	option igmp_snooping '1'
	option mld_snooping '1'
	option force_igmp_version '3'

config device
	option name 'eth1.1'
	option macaddr '94:83:c4:b0:97:44'

config interface 'lan'
	option device 'br-lan'
	option proto 'static'
	option ipaddr '192.168.8.1'
	option netmask '255.255.255.0'
	option ip6assign '60'
	option isolate '0'
	option multicast_querier '0'
	option igmp_snooping '1'
	option ip6hint '0000'
	option ip6ifaceid '::1'
	list ip6class 'wan6'
	list ip6class 'wwan6'
	list ip6class 'tethering6'
	list ip6class 'modem_2_1_6'
	list ip6class 'modem_1_1_6'

config device
	option name 'eth0'
	option macaddr '94:83:c4:b0:97:42'
	option mtu '1512'
	option mtu6 '1512'

config device
	option name 'eth0.6'
	option type '8021q'
	option ifname 'eth0'
	option vid '6'
	option mtu '1508'
	option mtu6 '1508'

config interface 'wan'
	option device 'eth0.6'
	option proto 'pppoe'
	option username 'internet'
	option password 'internet'
	option mtu '1500'
	option ipv6 'auto'
	option peerdns '1'
	option metric '10'
	option force_link '0'
	option classlessroute '0'
	option disabled '0'

config interface 'wan6'
	option interface 'wan'
	option proto 'dhcpv6'
	option reqprefix 'auto'
	option peerdns '1'
	option metric '20'
	option disabled '0'

config device
	option name 'eth0.4'
	option type '8021q'
	option ifname 'eth0'
	option vid '4'

config interface 'iptv'
	option proto 'dhcp'
	option ifname 'eth0.4'
	option delegate '0'
	option defaultroute '0'
	option peerdns '0'
	option metric '30'
	option vendorid 'IPTV_RG'

config route
	option interface 'iptv'
	option target '213.75.112.0/21'
	option gateway '10.220.0.1'

config switch
	option name 'switch1'
	option reset '1'
	option enable_vlan '1'

config switch_vlan
