	option disabled '1'
	option ip6prefix 'fd44:9f52:3f1f::/48'
	option ip6hint '0001'
	option ip6ifaceid '::1'
	option ip6class 'guest'

config interface 'wwan'
	option proto 'dhcp'
	option classlessroute '0'
	option metric '20'

config interface 'wwan6'
